<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	
	<class name="model.SellerInfovo" table="SellerInfo">
	
		<id name="siid" type="int" column="SINFOID">
			<generator class="increment"></generator>
		</id>
		<property name="vatid">
			<column name="VATID"></column>
		</property>
		
		<property name="pan">
			<column name="PAN"></column>
		</property>
		
		<property name="accno">
			<column name="ACCNO"></column>
		</property>		
		
		<many-to-one name="v" class="model.SellerLoginvo" column="SID"></many-to-one>
		
	</class>
</hibernate-mapping>